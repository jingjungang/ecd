<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:fitsSystemWindows="true"
    android:gravity="center" >

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@color/title1"
        android:gravity="center" >

        <LinearLayout
            android:id="@+id/title"
            android:layout_width="fill_parent"
            android:layout_height="75.0dip"
            android:orientation="horizontal" >

            <LinearLayout
                android:id="@+id/title1"
                android:layout_width="fill_parent"
                android:layout_height="75.0dip"
                android:background="@color/title1"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingBottom="10.0dip"
                android:paddingTop="10.0dip"
                android:visibility="visible" >

                <LinearLayout
                    android:layout_width="0.0dip"
                    android:layout_height="fill_parent"
                    android:layout_weight="2"
                    android:gravity="left|center" >

                    <ImageView
                        android:id="@+id/_back"
                        style="@style/btn_back"
                        android:background="@drawable/selector_return"
                        android:clickable="true" />
                </LinearLayout>

                <Button
                    android:id="@+id/fir_visit"
                    android:layout_width="0.0dip"
                    android:layout_height="50.0dip"
                    android:layout_margin="3.0dip"
                    android:layout_weight="1.0"
                    android:gravity="center"
                    android:background="@drawable/corner_choose_item_stoken"
                    android:text="第1次访视" />

                <TextView
                    android:id="@+id/tv_line1"
                    android:layout_width="0.0dip"
                    android:layout_height="10.0dip"
                    android:layout_weight="0.5"
                    android:background="@color/title2"
                    android:visibility="visible" />

                <Button
                    android:id="@+id/sec_visit"
                    android:layout_width="0.0dip"
                    android:layout_height="50.0dip"
                    android:layout_margin="3.0dip"
                    android:layout_weight="1.0"
                    android:background="@drawable/corner_green_"
                    android:text="第2次访视" />

                <TextView
                    android:id="@+id/tv_line2"
                    android:layout_width="0.0dip"
                    android:layout_height="10.0dip"
                    android:layout_weight="0.5"
                    android:background="@color/gray_color"
                    android:visibility="visible" />

                <Button
                    android:id="@+id/three_visit"
                    android:layout_width="0.0dip"
                    android:layout_height="50.0dip"
                    android:layout_margin="3.0dip"
                    android:layout_weight="1.0"
                    android:background="@drawable/corner_green_"
                    android:text="第3次访视" />

                <TextView
                    android:id="@+id/tv_line3"
                    android:layout_width="0.0dip"
                    android:layout_height="10.0dip"
                    android:layout_weight="0.5"
                    android:background="@color/gray_color"
                    android:visibility="visible" />

                <Button
                    android:id="@+id/four_visit"
                    android:layout_width="0.0dip"
                    android:layout_height="50.0dip"
                    android:layout_margin="3.0dip"
                    android:layout_weight="1.0"
                    android:background="@drawable/corner_green_"
                    android:text="第4次访视" />

                <TextView
                    android:id="@+id/tv_line4"
                    android:layout_width="0.0dip"
                    android:layout_height="10.0dip"
                    android:layout_weight="0.5"
                    android:background="@color/gray_color"
                    android:visibility="visible" />

                <Button
                    android:id="@+id/end_visit"
                    android:layout_width="0.0dip"
                    android:layout_height="50.0dip"
                    android:layout_margin="3.0dip"
                    android:layout_weight="1.0"
                    android:background="@drawable/corner_red"
                    android:text="中止访视"
                    android:textColor="@color/white_color" />

                <Button
                    android:id="@+id/finish_visit"
                    android:layout_width="0.0dip"
                    android:layout_height="50.0dip"
                    android:layout_margin="3.0dip"
                    android:layout_weight="1.0"
                    android:background="@drawable/corner_red"
                    android:text="实验完成"
                    android:textColor="@color/white_color"
                    android:visibility="gone" />

                <Button
                    android:layout_width="0.0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="2.0"
                    android:background="@color/transparent"
                    android:text="" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/title2"
                android:layout_width="fill_parent"
                android:layout_height="75.0dip"
                android:background="@color/title1"
                android:gravity="center"
                android:orientation="horizontal"
                android:visibility="gone"
                android:paddingBottom="10.0dip"
                android:paddingTop="10.0dip" >

                <LinearLayout
                    android:layout_width="0.0dip"
                    android:layout_height="fill_parent"
                    android:layout_weight="2"
                    android:gravity="left|center" >

                    <ImageView
                        android:id="@+id/_back1"
                        style="@style/btn_back"
                        
                        android:background="@drawable/selector_return"
                        android:clickable="true" />
                </LinearLayout>

                <Button
                    android:id="@+id/six_visit"
                    android:layout_width="0.0dip"
                    android:layout_height="50.0dip"
                    android:layout_margin="3.0dip"
                    android:layout_weight="1.0"
                    android:background="@drawable/corner_choose_item_stoken"
                    android:text="第5次访视" />

                <TextView
                    android:id="@+id/tv_line5"
                    android:layout_width="0.0dip"
                    android:layout_height="10.0dip"
                    android:layout_weight="0.5"
                    android:background="@color/title2"
                    android:visibility="visible" />

                <Button
                    android:id="@+id/seven_visit"
                    android:layout_width="0.0dip"
                    android:layout_height="50.0dip"
                    android:layout_margin="3.0dip"
                    android:layout_weight="1.0"
                    android:background="@drawable/corner_green_"
                    android:text="第6次访视" />

                <View
                    android:layout_width="0.0dip"
                    android:layout_height="50.0dip"
                    android:layout_weight="7.0" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_below="@+id/title"
            android:background="@color/title2" >

            <LinearLayout
                android:layout_width="100.0dip"
                android:layout_height="fill_parent"
                android:layout_marginTop="2dp"
                android:background="@color/title2"
                android:gravity="center" >

                <ListView
                    android:id="@+id/lv1"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_gravity="center"
                    android:layout_marginTop="2.0dip"
                    android:divider="@android:color/transparent"
                    android:gravity="center"
                    android:scrollbars="none" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_marginTop="2.0dip"
                android:gravity="center" >

                <LinearLayout
                    android:layout_width="100.0dip"
                    android:layout_height="fill_parent"
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/tv_visit"
                        android:layout_width="fill_parent"
                        android:layout_height="50.0dip"
                        android:background="@color/title1"
                        android:gravity="center"
                        android:text="第1次访视"
                        android:textColor="@color/blue_color" />

                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="1.0dip"
                        android:background="@color/white_color" />

                    <ListView
                        android:id="@+id/lv2"
                        android:layout_width="100.0dip"
                        android:layout_height="fill_parent"
                        android:layout_gravity="center"
                        android:background="@color/title1"
                        android:cacheColorHint="#00000000"
                        android:divider="@android:color/transparent"
                        android:gravity="center"
                        android:scrollbars="none" />
                </LinearLayout>

                <fragment
                    android:id="@+id/main_fragment"
                    android:name="com.ukang.clinic.main.Frg_Menu"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginTop="5dp"
                    android:background="@color/white_color" />
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/index"
        android:layout_width="fill_parent"
        android:layout_height="50.0dip"
        android:layout_marginTop="55.0dip"
        android:orientation="horizontal" >

        <View
            android:layout_width="0.0dip"
            android:layout_height="wrap_content"
            android:layout_weight="2.0" />

        <LinearLayout
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_margin="3.0dip"
            android:layout_weight="1.0"
            android:gravity="center" >

            <ImageView
                android:id="@+id/img1"
                android:layout_width="20.0dip"
                android:layout_height="20.0dip"
                android:background="@drawable/circle" />
        </LinearLayout>

        <TextView
            android:layout_width="0.0dip"
            android:layout_height="10.0dip"
            android:layout_weight="0.5" />

        <LinearLayout
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_margin="3.0dip"
            android:layout_weight="1.0"
            android:gravity="center" >

            <ImageView
                android:id="@+id/img2"
                android:layout_width="20.0dip"
                android:layout_height="20.0dip"
                android:background="@drawable/circle"
                android:visibility="invisible" />
        </LinearLayout>

        <TextView
            android:layout_width="0.0dip"
            android:layout_height="10.0dip"
            android:layout_weight="0.5" />

        <LinearLayout
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_margin="3.0dip"
            android:layout_weight="1.0"
            android:gravity="center" >

            <ImageView
                android:id="@+id/img3"
                android:layout_width="20.0dip"
                android:layout_height="20.0dip"
                android:background="@drawable/circle"
                android:visibility="invisible" />
        </LinearLayout>

        <TextView
            android:layout_width="0.0dip"
            android:layout_height="10.0dip"
            android:layout_weight="0.5" />

        <LinearLayout
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_margin="3.0dip"
            android:layout_weight="1.0"
            android:gravity="center" >

            <ImageView
                android:id="@+id/img4"
                android:layout_width="20.0dip"
                android:layout_height="20.0dip"
                android:background="@drawable/circle"
                android:visibility="invisible" />
        </LinearLayout>

        <TextView
            android:layout_width="0.0dip"
            android:layout_height="10.0dip"
            android:layout_weight="0.5" />

        <LinearLayout
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_margin="3.0dip"
            android:layout_weight="1.0"
            android:gravity="center" >

            <ImageView
                android:id="@+id/img5"
                android:layout_width="20.0dip"
                android:layout_height="20.0dip"
                android:background="@drawable/circle"
                android:visibility="invisible" />
        </LinearLayout>

        <View
            android:layout_width="0.0dip"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:background="@color/transparent" />
    </LinearLayout>

</FrameLayout>